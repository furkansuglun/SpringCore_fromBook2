<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
  	
  
 
	<bean id="beanOgrenci" class="_04.Injecting.Collection.Ogrenci"
		scope="singleton">
		<property name="ogrenciList">
			<list>
				<value>Furkan Süğlün</value>
				<value>Emin Erdar</value>
				<value>Emre Yılmaz</value>
			</list>
		</property>
		
		<property name="ogrenciSet">
			<set>
				<value>Furkan Süğlün</value>
				<value>Emin Erdar</value>
				<value>Emre Yılmaz</value>
			</set>
		</property>
		
		<property name="ogrenciMap">
			<map>
				<entry key="1" value="Furkan Süğlün"/>
				<entry key="2" value="Emin Erdar"/>
				<entry key="3" value="Emre Yılmaz"/>
			</map>
		</property>
		
		<property name="ogrenciProp">
			<props>
				<prop key="IT">Furkan Süğlün</prop>
				<prop key="Aircraft Management">Emin Erdar</prop>
				<prop key="IT Analist">Emre Yılmaz</prop>
			</props>
		</property>
	</bean>
</beans>